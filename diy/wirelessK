
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/a000000.wifi'
	option band '2g'
	option legacy_rates '1'
	option mu_beamformer '0'
	option country 'US'
	option channel '8'
	option txpower '30'
	option noscan '1'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'KKT'
	option key 'abc5124937,'
	option hidden '1'
	option encryption 'psk-mixed+ccmp'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc/a800000.wifi'
	option band '5g'
	option legacy_rates '1'
	option mu_beamformer '0'
	option txpower '30'
	option country 'US'
	option htmode 'VHT80'
	option channel '157'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'KKT'
	option key 'abc5124937,'
	option encryption 'psk-mixed+ccmp'

